{
    "name": "zientalak/devicedetector",
    "description": "Device Detector for PHP",
    "type": "library",
    "minimum-stability": "stable",
    "license": "MIT",
    "authors": [
        {
            "name": "Damian Zientalak",
            "email": "damian.zientalak@gmail.com"
        }
    ],
    "config": {
        "bin-dir": "bin"
    },
    "require": {
        "php": ">=5.3.3",
        "doctrine/common" : "2.5.*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.3.*",
        "phpspec/phpspec": "2.2.*",
        "henrikbjorn/phpspec-code-coverage": "1.0.*",
        "symfony/yaml" : "2.6.*",
        "symfony/finder" : "2.6.*",
        "symfony/var-dumper" : "2.6.*",
        "symfony/stopwatch" :  "2.6.*",
        "doctrine/cache" : "1.4.*",
        "phpmd/phpmd": "@stable",
        "sebastian/phpcpd": "@stable",
        "squizlabs/php_codesniffer" : "@stable"
    },
    "suggest": {
        "doctrine/cache" : "Can be used for caching devices and rules."
    },
    "autoload": {
        "psr-4" : {
            "DeviceDetectorIO\\DeviceDetector\\" : "src/DeviceDetectorIO/DeviceDetector",
            "DeviceDetectorIO\\DeviceDetector\\Tests\\" : "tests/DeviceDetectorIO/DeviceDetector"
        }
    }
}